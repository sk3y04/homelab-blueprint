# Jellyfin configuration (example)
# Copy to .env and adjust.

TZ=UTC
JELLYFIN_PUBLISHED_SERVER_URL=https://jellyfin.example.com

# Volumes
JELLYFIN_CONFIG_DIR=/path/to/jellyfin/config
JELLYFIN_CACHE_DIR=/path/to/jellyfin/cache
MEDIA_ROOT_DIR=/path/to/media
TMP_DIR=/tmp

# Ports
JELLYFIN_HTTP_PORT=8096
